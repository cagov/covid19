<!-- begin video section -->
<video-section>

<section class="video trigger" role="region" aria-labelledby="video-resources">
<div class="video-wrap">

	<h3 class="video-headline text-center"><a class="video-headline-link" href="/" id="video-resources">{{varVideoTitle}}</a></h3>
	<div class="video-grid">

		<div class="video-grid-col">
			<article class="video-item transition-0_3 transform-scale-hover" data-video="{{ varVideoID }}">
				<a class="video-modal-open video-thumb-link w-100 w-lg-40" data-video="{{ varVideoID }}" href="https://youtu.be/{{ varVideoID }}">
          <figure class="video-thumb-photo">
            <div class="video-thumb-play-button">
              <svg class="video-thumb-play-button-icon" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 50 50">
                <path d="M43.3,25L0,50V0L43.3,25z"></path>
              </svg>
            </div>
            <img class="video-thumb-photo-img" src="https://files.covid19.ca.gov/img/homepage-video-thumb--en.jpg" alt="wear a mask">
          </figure>
        </a>
			</article>
		</div>

		<div class="video-grid-col">
			<div class="video-grid-more">

          <div class="video-text pl-md-5 pt-3 pt-lg-0">
            <h4 class="font-size-2em color-purple font-weight-bold">{{varVideoWearAMask}}</h4>
            <p class="lead mt-4">{{varVideoP1}}</p>

						<a class="button trigger video-modal-open cwdsarrow-parent" href="https://youtu.be/{{ varVideoID }}" data-video="{{ varVideoID }}">
							<div class="button-label">{{varVideoWatchTheVideo}}</div>
							<cwds-arrow data-class-prefix="watch-video" data-class-list="cwdsarrow-size-md cwdsarrow-idle-black cwdsarrow-hover-darkblue cwdsarrow-focus-darkblue"></cwds-arrow>

						</a>

          </div>

			</div>
		</div>

	</div>

</div>
</section>

</video-section>
<!-- end video section -->



<!-- PEOPLE WANTS TO KNOW SECTION -->
<faq-section>
<section class="faq-twitter" role="region" aria-labelledby="want-to-know">
	<div class="header-wrap">
	<h4 class="faq-headline"><a class="faq-headline-link" href="/" id="want-to-know">{{varPeopleWantToKnowTitle}}</a></h4>
	</div>

	<div class="faq-twitter-wrap">

{% set JSONraw %}{% include langIncludeFolder+"people-want-to-know" + langFilePostfix + ".json" %}{% endset %}


{% set JSON = JSONraw | jsonparse %}
{% set questionLinks1 = JSON.Table1 %}
{% set questionLinks2 = JSON.Table2 %}

	<div class="faq trigger">
		{% for item in questionLinks1 %}
		<article class="faq-item trigger">    
			<a class="faq-item-link cwdsarrow-parent" href="{{ item.url }}">
				<h4 class="faq-item-headline">{{ item.text | safe }}</h4>
				<cwds-arrow data-class-prefix="faq" data-class-list="cwdsarrow-size-sm cwdsarrow-idle-orange cwdsarrow-hover-darkblue cwdsarrow-focus-darkblue"></cwds-arrow>
			</a>
		</article>
 		{% endfor %}
	</div>

	<div class="faq trigger">
		{% for item in questionLinks2 %}
		<article class="faq-item trigger">
			<a class="faq-item-link cwdsarrow-parent" href="{{ item.url }}">
				<h4 class="faq-item-headline">{{ item.text | safe }}</h4>
				<cwds-arrow data-class-prefix="faq" data-class-list="cwdsarrow-size-sm cwdsarrow-idle-orange cwdsarrow-hover-darkblue cwdsarrow-focus-darkblue"></cwds-arrow>
			</a>	
		</article>
	{% endfor %}
	</div>

</div>
</section>
</faq-section>

  {% include "./news-feed-home.html" %}
</main>
{% include "footer.njk" %}

<!-- begin video overlay -->
<div id="video-modal" class="video-modal" style="display: none">
  <div class="video-modal-content">
    <button class="video-modal-close modal_close">
      <svg class="video-modal-close-svg" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 640 640">
