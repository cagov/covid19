	<!-- FOR BUSINESSES SECTION -->
<learn-more-section>
<section class="faq-twitter pb-4">
	<h2 class="text-center color-purple mt-0">For businesses</h2>
	<div class="faq-twitter-wrap pb-5">
	{% set JSONgovraw %}{% include "../wordpress-posts/common-page-labels.json" %}{% endset %}
        {% set JSONgov = JSONgovraw | jsonparse %}
        {%- set RowsGov = JSONgov.Table4 -%}
        {% for item in RowsGov %}
		<div class="faq trigger">
			<div class="arrow-links">
			<article>    
				<a class="link-arrow-blue" href="{{item["URL"]}}">
				<div class="link-arrow-label">
					{{item["Link Title"]}}
				</div>
				<cagov-arrow></cagov-arrow>
				</a>
			</article>
			</div>
		</div>
		 {% endfor %}
	</div>
</section>
</learn-more-section>


<!-- begin video section -->
	<video-section>

	<section class="video trigger bg-lightblue" role="region" aria-labelledby="video-resources">
		<div class="video-wrap text-center">

			{% set JSONVidraw %}{% include "../wordpress-posts/video-section.json" %}{% endset %}
			{% set JSONVid = JSONVidraw | jsonparse %}
			{%- set RowsVid = JSONVid.Table1 -%}
			{% for item in RowsVid %}
			{% if item.Active == "yes" %}
			<div class="container">
			<div class="row">
			<div class="col-lg-8 mx-auto text-center">
			<h2 class="color-purple mt-0 mb-0 pb-2" id="video-resources">{{item["Video Title"]}}</h2>
			<p class="mb-3 text-300">{{item["Description"]}}</p>
				<article class="video-item mx-auto mt-3" data-video="{{item["VideoID"]}}">
					<a class="video-modal-open video-thumb-link w-100 w-lg-40" data-video="{{item["VideoID"]}}" href="https://{{item["VideoURL"]}}">
						<figure class="video-thumb-photo">
							<div class="video-thumb-play-button" role="button" tabindex="0" aria-label="Watch video">
								<svg class="video-thumb-play-button-icon" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 50 50">
									<path class="st0" d="M0,45.9V4.4c0-3.3,2-4.2,4.9-2.5l32.9,19c4.7,2.6,4.8,5,0.6,7.5L5,47.6C1.7,49.6,0,48.5,0,45.9z"/>
								</svg>
							</div>
							<img class="video-thumb-photo-img" src="https://{{item["ImageURL"]}}" alt="{{item["ImageALT"]}}">
						</figure>
					</a>
				</article>

				<a href="https://{{item["ButtonURL"]}}" class="link-arrow-blue mt-4" data-video="{{item["VideoID"]}}">
					<div class="link-arrow-label">{{item["ButtonText"]}}</div>
					<cagov-arrow></cagov-arrow>
				</a>

		
				</div>
				</div>
				</div>

				{% endif %}
				{% endfor %}
				
		</div>
	</section>
</video-section>
