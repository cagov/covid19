<hero-stats>
	<div class="bg-darkblue full-bleed mb-0">
  <div class="container py-2">
    <div class="row pb-4">
				<div class="hero-stats" role="region" aria-labelledby="tracking-covid">
					<h2 class="color-orange text-center m-0 pb-2 pt-3" id="tracking-covid">{{translatedLabels.varStatsHeader}}</h2>

		{%-set _varCasesDateAccurate_ = stateDashboardCasesUpdateTime.PUBLISH_DATE | formatDate2(true,tags,0)-%}
		{%-set _varCasesDateRounded_ = stateDashboardCasesUpdateTime.ROUNDED_PUBLISH_DATE | formatDate2(true,tags,0)-%}
		{%-set _varCasesDateReport_ = dailyStatsV2.data.cases.DATE | formatDate2(false, tags,0)-%}

		{%-set _varVaxDateAccurate_ = stateDashboardVaxUpdateTime.PUBLISH_DATE | formatDate2(true,tags,0)-%}
		{%-set _varVaxDateRounded_ = stateDashboardVaxUpdateTime.ROUNDED_PUBLISH_DATE | formatDate2(true,tags,0)-%}
		{%-set _varVaxDateReport_ = dailyStatsV2.data.vaccinations.DATE | formatDate2(false, tags,0)-%}

        {%- set _deltaHours_ = '' | dateDeltaHours(stateDashboardCasesUpdateTime.PUBLISH_DATE,stateDashboardVaxUpdateTime.PUBLISH_DATE) -%}
        {%- set _varStatDateLaterAccurate_ = '' | latestDate(stateDashboardCasesUpdateTime.PUBLISH_DATE,stateDashboardVaxUpdateTime.PUBLISH_DATE) | formatDate2(true,tags,0) -%}
        {%- set _varCommonDateReport_ = _varCasesDateReport_ -%}
        {%- if _deltaHours_ < 1.0 and _varCasesDateReport_ == _varVaxDateReport_ -%}
		HELLO!!
					<div class="text-sm text-center text-300">
						{{- translatedLabels.varUpdateText | replace("[UpdatedDate]", _varStatDateLaterAccurate_) | replace("[DataDate]",_varCommonDateReport_) -}}
					</div>

		{%- else -%}
		HELLO2!!
					<div class="text-sm text-center text-300">
						{{- translatedLabels.varUpdateTextVax | replace("[VaxUpdatedDate]", _varVaxDateAccurate_) | replace("[VaxDataDate]",_varVaxDateReport_) -}}
					</div>
					<div class="text-sm text-center text-300">
						{{- translatedLabels.varUpdateTextCaes | replace("[CasesUpdatedDate]", _varCasesDateAccurate_) | replace("[CasesDataDate]",_varCasesDateReport_) -}}
					</div>
		{%- endif -%}

					<!----- Begin Summary Boxes -->
					<div class="row d-flex justify-content-md-center summary-boxes">
						<!-- Blue Summary Box 1 -->						
						<div class="col-md-6 col-lg-3 px-10px">							
							<div class="bg-blue bd-rounded-10 px-6px py-2 text-center mt-3 summary-box d-flex flex-column">
								<div class="h5 text-uppercase color-white line-height-1-2 mt-1 mb-auto">{{ translatedLabels.varVaccinesAdministered }}</div>
								<div id="total-vaccines-number" class="text-white font-size-1-2em mb-2"><strong>{{_varStatVaccines_}}</strong>
								<span class="text-300">{{translatedLabels.varTotal}}</span>
								</div>
								<div id="total-vaccines-today" class="text-white font-size-1-2em mb-2">
								<strong>{{_varDosesDailyAverage_}}</strong> <span class="text-300">{{ translatedLabels.varDailyAvg }}</span>
								</div>
								<div class="text-white text-xs mb-2">
									<div>
										<span class="text-300"><strong>{{_varTotalVaxedPercent_}}%</strong> of population vaccinated</span>
									</div>
									<div>
										<span class="text-300">(5+ with at least one dose)</span>
									</div>
								</div>
								<!--Chart and Label -->
								<div class="mt-auto px-2">
   								    <div class="sparkline-vax"></div>
									<div class="text-white text-xs mt-1">
										<span class="text-300">{{ translatedLabels.varPast8Weeks }}</span>
