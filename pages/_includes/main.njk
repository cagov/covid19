{%- set langFilePostfix = tags | langFilePostfix -%}
{%- set langIncludeFolder="../translated-posts/" if langFilePostfix else "../wordpress-posts/" -%}

{%- set lastStatDate = 0|_statsdata_ + "-" + 1|_statsdata_ + "-" + 2|_statsdata_+"T18:00:00Z" -%}
{%- set FormattedUpdateText = varUpdateText | replace("[UpdatedDate]",lastStatDate | formatDate2(true,tags)) | replace("[DataDate]",lastStatDate | formatDate2(false,tags,-1))-%}

{%- set JSONraw %}{% include "../wordpress-posts/footer-data" + langFilePostfix + ".json" -%}{%- endset -%}
{%- set JSON = JSONraw | jsonparse -%}
{%- set Terms = JSON.Table3 -%}

<!DOCTYPE html>
<html {{ tags | htmllangattributes | safe }}>
  <head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <meta name="description" content="{{ meta | safe }}"/>
    <meta name="author" content="{{ author | safe }}"/>
    <meta
      name="keywords"
      content="covid-19 california, covid-19 ca, covid-19 volunteer, covid-19 testing, covid-19 testing costs, covid-19 testing california, covid-19 toolkit, stay home save lives, california health corps, covid-19 immigrants, covid-19 financial help, covid-19 medi-cal, covid-19 healthcare, covid-19 alerts"
/>
    <title>{{ title | safe }}</title>

    <!-- combined social media meta -->
    <meta content="{{ title | safe }}" property="og:title"/>
    <meta
      content="{{ meta | safe }}"
      property="og:description"
/>
    <meta
      content="https://covid19.ca.gov/img/awareness/caforall_covid_endcards_v4_english.jpg"
      property="og:image"
/>
    <meta content="https://covid19.ca.gov/" property="og:url"/>
    <meta name="twitter:card" content="summary_large_image"/>
    <link rel="preconnect" href="https://www.google-analytics.com/">
    <link href="https://covid19.ca.gov{{ page.url }}" rel="canonical">

    {% include "favicons.njk" %}
    <link rel="manifest" href="/img/manifest.json"/>
    <meta name="msapplication-tilecolor" content="#ffffff"/>
    <meta name="msapplication-tileimage" content="img/ms-icon-144x144.png"/>
    <meta name="theme-color" content="#ffffff"/>
    {% for item in page | getAltPageRows -%}
      <link rel="alternate" hreflang="{{item.langcode}}" href="https://covid19.ca.gov{{item.url}}">
    {%- endfor %}
    {% if env.dev %}
      <link rel="stylesheet" type="text/css" href="/css/build/development.css">
    {% else %}
      <style>
        {% include "../_buildoutput/home.css" %}
      </style>
    {% endif %}
		<script>const videoID='{{varVideoID}}';</script>
  </head>
  <body>
    {% include "header.njk" %}
    <main id="main">

<hero-stats>
   <section class="hero jumbo-img trigger">
	<div class="hero-wrap">

		<div class="hero-text" role="region" aria-labelledby="hero-text">
			<div class="hero-text-align">				
				<h1 class="hero-headline">
					<a class="hero-headline-link" href="{{ "symptoms-and-risks/" | toTranslatedPath(tags) }}">
						<span class="hero-headline-background" id="hero-text">
							{{varHeroTitle}}
						</span>
					</a>
				</h1>

			<a class="hero-button button orange-button cwdsarrow-parent" href="{{ "symptoms-and-risks/" | toTranslatedPath(tags) }}">
				<div class="button-label">
					<span>
						<p>{{ varHeroP1 }}</p>
						<p>{{ varHeroP2 }}</p>
					</span>
				</div>
					<cwds-arrow data-class-prefix="hero" data-class-list="cwdsarrow-size-md cwdsarrow-idle-lightblue cwdsarrow-hover-white cwdsarrow-focus-orange"></cwds-arrow>
			</a>
		</div>
		

		</div>

		<div class="hero-stats" role="region" aria-labelledby="tracking-covid">
			<h2 class="hero-stats-headline" id="tracking-covid">{{varStatsHeader}}</h2>
			<div class="hero-stats-subheadline">{{FormattedUpdateText}}</div>

			<div class="hero-stats-slider">

				<div class="hero-stats-slide" aria-hidden="true" aria-labelledby="cumulative-stats">

					<div class="hero-stats-row">
						<div class="hero-stats-row-label">
							<span class="hero-stats-row-label-text">{{varTotalCases}}</span>
						</div>
						<div class="hero-stats-row-figure">{{3|_statsdata_}}</div>
						<div class="hero-stats-row-details">
							+{{4|_statsdata_}}% 
						</div>
					</div>

					<div class="hero-stats-row">
						<div class="hero-stats-row-label">
							<span class="hero-stats-row-label-text">{{varTotalDeaths}}</span>
						</div>
						<div class="hero-stats-row-figure">{{5|_statsdata_}}</div>
						<div class="hero-stats-row-details">
							+{{6|_statsdata_}}%
						</div>
					</div>

					<div class="hero-stats-row">
						<div class="hero-stats-row-label">
							<span class="hero-stats-row-label-text">{{varTestsReported}}</span>
						</div>
						<div class="hero-stats-row-figure">{{7|_statsdata_}}</div>
						<div class="hero-stats-row-details">
						
						</div>
					</div>
				</div>

			</div>
			<div class="hero-stats-footer">
				<a class="hero-stats-footer-link mt-2" href="{{ 'safer-economy/' | toTranslatedPath(tags) }}">{{varCountyMap}}</a>
