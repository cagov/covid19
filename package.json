    "babel-loader": "^8.1.0",
    "clean-css": "^4.2.3",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^9.0.0",
    "imagemin-webp": "^6.0.0",
    "jsdom": "^16.2.2",
    "md5": "^2.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
